{
    "name": "Eye Break Reminder",
    "description": "Reminder to take eye breaks often",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["alarms", "notifications"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_title": "Drink Water Event"
    },
    "content_scripts" : [
      {
        "js": ["content.js"],
        "matches": ["<all_urls>"]
      }
    ],
    "icons": {
      "16": "images/eye16.png",
      "32": "images/eye32.png",
      "48": "images/eye48.png",
      "128": "images/eye128.png"
    }
  }